{
    "name": "act-training/laravel-model-versions",
    "description": "A Laravel package for automatic model versioning with selective attribute tracking and restore capabilities",
    "keywords": ["laravel", "model", "versioning", "eloquent", "audit", "tracking"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ACT Training",
            "email": "dev@acttraining.com.au"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/database": "^12.0",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.25",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpunit/phpunit": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "ActTraining\\LaravelModelVersions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActTraining\\LaravelModelVersions\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ActTraining\\LaravelModelVersions\\LaravelModelVersionsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
